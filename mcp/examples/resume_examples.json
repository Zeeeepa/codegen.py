{
  "name": "codegenapi_resume",
  "description": "Resume an existing agent run",
  "examples": [
    {
      "title": "Basic resume",
      "arguments": {
        "agent_run_id": 12345,
        "query": "Please also include error handling"
      }
    },
    {
      "title": "Resume with task context",
      "arguments": {
        "agent_run_id": 12345,
        "task": "ANALYZE",
        "query": "Focus on security vulnerabilities in the authentication flow"
      }
    },
    {
      "title": "Resume and wait for completion",
      "arguments": {
        "agent_run_id": 12345,
        "query": "Complete the analysis with performance considerations",
        "wait_for_completion": true
      }
    }
  ]
}

